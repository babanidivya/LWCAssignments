<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Check_Length</fullName>
    <active>true</active>
    <description>length of contact should be 10</description>
    <errorConditionFormula>LEN(Student_Contact__c)!=10</errorConditionFormula>
    <errorDisplayField>Student_Contact__c</errorDisplayField>
    <errorMessage>enter only 10 digits</errorMessage>
</ValidationRule>
